{
    "name": "puwnz/google-maps-bundle",
    "description": "Bundle Symfony to use Google Maps Lib",
    "type": "symfony-bundle",
    "keywords": [ "google", "maps", "map", "symfony", "vendor", "bundle" ],
    "authors": [
        {
            "name": "Guillaume D.",
            "email": "guigz-style@live.fr"
        }
    ],
    "require": {
        "php": ">=7.3",
        "puwnz/google-maps-lib": "^1.0",
        "symfony/http-kernel": "^4.4|^5.0",
        "symfony/config": "^7.1.1",
        "symfony/dependency-injection": "^4.4|^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "friendsofphp/php-cs-fixer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Puwnz\\GoogleMapsBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Puwnz\\GoogleMapsBundle\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "tests" : [
            "php vendor/bin/phpunit --coverage-text"
        ]
    },
    "license": "MIT"
}
